<!--index.wxml-->
<view>
  <view class='nickname'>
    玩家：{{ userinfo.nickName }}
  </view>
  <view class='nickname'>
    <image class='userimg' src='{{ userinfo.avatarUrl }}'></image>
  </view>   
</view>
<view class='container' bindtouchstart='start' bindtouchmove='move' bindtouchend='end'>
  <label class='item d{{item}}' wx:for='{{mydata}}'>
    {{ item ? item : ''}}
  </label>
</view>
<modal title='提示' confirm-text='确定' no-cancel hidden='{{ gameover.status }}' bind-confirm='restart'>
  {{ gameover.message }}
</modal>
